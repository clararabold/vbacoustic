{
  "name": "vbacoustic-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@vbacoustic/web-ui",
    "build": "npm run build --workspaces",
    "build:lib": "npm run build --workspace=@vbacoustic/lib",
    "build:ui": "npm run build --workspace=@vbacoustic/web-ui",
    "test": "npm run test --workspace=@vbacoustic/lib",
    "test:coverage": "npm run test:coverage --workspace=@vbacoustic/lib",
    "lint": "npm run lint --workspaces",
    "deploy": "npm run deploy --workspace=@vbacoustic/web-ui",
    "publish:lib": "npm publish --workspace=@vbacoustic/lib"
  },
  "keywords": [
    "acoustics",
    "building",
    "sound",
    "DIN4109",
    "ISO12354",
    "react",
    "typescript"
  ],
  "author": "VBAcoustic Implementation Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
